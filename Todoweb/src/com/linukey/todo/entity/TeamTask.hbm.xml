<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.linukey.todo.entity">
    <class name="TeamTask" table="teamtask">
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="title" />
        </property>
        <property name="content" type="java.lang.String">
            <column name="content" />
        </property>
        <property name="starttime" type="java.lang.String">
            <column name="starttime" />
        </property>
        <property name="endtime" type="java.lang.String">
            <column name="endtime" />
        </property>
        <property name="clocktime" type="java.lang.String">
            <column name="clocktime" />
        </property>
        <property name="projectId" type="java.lang.String">
            <column name="projectId" />
        </property>
        <property name="state" type="java.lang.String">
            <column name="state" />
        </property>
        <property name="isdelete" type="java.lang.String">
            <column name="isdelete" />
        </property>
        <property name="teamId" type="java.lang.String">
            <column name="teamId" />
        </property>
        <property name="selfId" type="java.lang.String">
            <column name="selfId" />
        </property>
    </class>
</hibernate-mapping>